{
    "name": "propelcog/craft-order-import",
    "description": "Plugin to extend feedme to import orders",
    "type": "craft-plugin",
    "version": "4.3.7",
    "support": {
        "email": "staff@propelcog.com"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.4.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "propelcog\\craftorderimport\\": "src/"
        }
    },
    "extra": {
        "handle": "order-import",
        "name": "Order Import",
        "developer": "propelcog",
        "documentationUrl": ""
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

